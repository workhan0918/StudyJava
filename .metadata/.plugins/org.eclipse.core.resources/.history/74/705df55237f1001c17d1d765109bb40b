CREATE TABLE Customer (
	name		VARCHAR(20)		NOT NULL,
	ssn			VARCHAR(14)		NOT NULL,
	phone		VARCHAR(14)		NOT NULL,
	CustomerId	VARCHAR(16)		NOT NULL,
	passwd		VARCHAR(16)		NOT NULL,
);
