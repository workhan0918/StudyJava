package com.varxyz.jv200.mod009;

import java.util.HashMap;
import java.util.Map;

public class MapExample {
	public static void main(String[] args) {
		Map<Integer, String> map = new HashMap<Integer, String>(); // int는 객체가 아니기 때문에 Integer로 사용
		map.put(new Integer(1), "돈까스");
		map.put(new Integer(2), "우동");
		map.put(new Integer(3), "라면");
	}
}
