package com.varxyz.jv200.mod007;

public class Book extends Libary{
	private int serial;
	private int name;
	public Book (int serial, String name) {
		
	}
	
	public Book addBook(Book bookInfo) {
		return bookInfo;
	}

	public int getSerial() {
		return serial;
	}

	public void setSerial(int serial) {
		this.serial = serial;
	}
	
}
